{
  "name": "routish",
  "version": "0.1.0",
  "description": "Type-safe routes, no strings attached. A tiny, fully type-safe route builder for TypeScript.",
  "keywords": [
    "router",
    "routes",
    "routing",
    "typescript",
    "type-safe",
    "url",
    "path",
    "react-router",
    "vue-router"
  ],
  "license": "MIT",
  "author": "Yanief Latief",
  "repository": {
    "type": "git",
    "url": "https://github.com/yanief/routish"
  },
  "bugs": {
    "url": "https://github.com/yanief/routish/issues"
  },
  "homepage": "https://github.com/yanief/routish#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "sideEffects": false
}
